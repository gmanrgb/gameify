{
  "name": "questlog",
  "version": "1.0.0",
  "private": true,
  "description": "Local-first goal gamification app - All data stored in browser localStorage",
  "scripts": {
    "dev": "pnpm --filter @questlog/web dev",
    "build": "pnpm --filter @questlog/shared build && pnpm --filter @questlog/web build",
    "preview": "pnpm --filter @questlog/web preview",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.14.0"
}
